{
	"name": "rmcgirr83/prunepms",
	"type": "phpbb-extension",
	"description": "An extension which allows you to delete older private messages in your phpBB forum. Found under `Users and Groups` tab => users.",
	"homepage": "https://phpbb.com",
	"version": "1.0.0",
	"keywords": ["phpbb", "extension", "cleaner"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Rich McGirr (RMcGirr83)",
			"role": "developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Prune PMs",
		"soft-require": {
			"phpbb/phpbb": ">=3.2"
		}
	}
}
